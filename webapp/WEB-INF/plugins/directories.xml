<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<plug-in>
    <name>directories</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>directories.plugin.description</description>
    <provider>directories.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr/</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2019 Mairie de Paris</copyright>
    <db-pool-required>1</db-pool-required>
    <core-version-dependency>
        <min-core-version>7.0.0</min-core-version>
        <max-core-version />
    </core-version-dependency>
    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>DIRECTORIES_MANAGEMENT</feature-id>
            <feature-title>directories.adminFeature.DirectoriesManager.name</feature-title>
            <feature-description>directories.adminFeature.DirectoriesManager.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/directories/ManageDirectories.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
        </admin-feature>
    </admin-features>
    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>directories</application-id>
            <application-class>fr.paris.lutece.plugins.directories.web.DirectoriesApp</application-class>
        </application>
    </applications>
    <!-- Search Indexer Service -->
    <search-indexers></search-indexers>
    <!-- Portlet parameters -->
    <portlets></portlets>
</plug-in>